{
  "name": "@aperturerobotics/protobuf-project",
  "version": "0.0.0",
  "description": "Example of a Starpc RPC project.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json --outDir ./dist/",
    "check": "bun run typecheck",
    "typecheck": "tsc --noEmit",
    "deps": "depcheck --ignore-patterns=package.json --ignores depcheck,rimraf,prettier,typescript,starpc,@go/github.com,@aptre/common,@eslint/js,@typescript-eslint/eslint-plugin,eslint-config-prettier,eslint-plugin-react-hooks,eslint-plugin-unused-imports,globals",
    "codegen": "bun run gen",
    "ci": "bun run build && bun run lint:js && bun run lint:go",
    "format": "bun run format:js && bun run format:go",
    "format:js": "prettier --write './!(vendor|dist)/**/(*.ts|*.tsx|*.js|*.html|*.css)'",
    "format:go": "bun run go:aptre -- format",
    "gen": "bun run go:aptre -- generate",
    "gen:force": "bun run go:aptre -- generate --force",
    "test": "bun run go:aptre -- test && bun run check",
    "test:js": "echo No JS tests.",
    "lint": "bun run lint:go && bun run lint:js",
    "lint:go": "bun run go:aptre -- lint",
    "lint:js": "eslint ./",
    "precommit": "bun run format",
    "prepare": "go mod vendor && rimraf ./.tools",
    "go:aptre": "go run -mod=mod github.com/aperturerobotics/common/cmd/aptre"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@aptre/common": "^0.30.3",
    "depcheck": "^1.4.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.7",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aptre/it-ws": "^1.0.0",
    "@aptre/protobuf-es-lite": "^1.0.1",
    "@eslint/js": "^10.0.1",
    "starpc": "^0.46.2"
  },
  "resolutions": {
    "@aptre/protobuf-es-lite": "1.0.1"
  }
}
